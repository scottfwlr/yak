# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
bundle.js
bundle.js.map
/node_modules
/yarn-error.log
npm-debug.log*
.DS_Store
.byebug_history
/public/assets
